type User {
  id: ID! @id
  first_name: String! @default(value: "Unknown")
  last_name: String
  username: String! @unique
  password: String!
  email: [String!]! @unique @scalarList(strategy: RELATION)
  phone: [String]! @unique @scalarList(strategy: RELATION)
  created_at: DateTime! @created_at
  updated_at: DateTime! @updated_at
}
